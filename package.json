{
	"name": "auditphobia",
	"private": false,
	"description": "Check security vulnerabilites of your npm packages",
	"license": "MIT",
	"sideEffects": false,
	"scripts": {
		"build": "npm run build:css && remix build",
		"build:css": "tailwindcss -m -i ./app/styles/base/tailwind.css -o app/styles/tailwind.css",
		"dev": "concurrently \"npm run dev:css\" \"remix dev\"",
		"dev:css": "tailwindcss -w -i ./app/styles/base/tailwind.css -o app/styles/tailwind.css",
		"postinstall": "remix setup node",
		"start": "cross-env NODE_ENV=production remix-serve build",
		"lint": "eslint .",
		"lint:fix": "yarn lint --fix",
		"prepare": "husky install",
		"test": "jest",
		"test:watch": "jest --watch"
	},
	"dependencies": {
		"@remix-run/react": "^1.2.2",
		"@remix-run/serve": "^1.2.2",
		"auditphobia-gadget": "0.0.1",
		"cross-env": "^7.0.3",
		"node-cache": "^5.1.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"remix": "^1.2.2"
	},
	"devDependencies": {
		"@remix-run/dev": "^1.2.2",
		"@tailwindcss/typography": "^0.5.2",
		"@testing-library/jest-dom": "^5.16.2",
		"@testing-library/react": "^12.1.4",
		"@types/jest": "^27.4.1",
		"@types/react": "^17.0.24",
		"@types/react-dom": "^17.0.9",
		"@typescript-eslint/eslint-plugin": "^5.14.0",
		"@typescript-eslint/parser": "^5.14.0",
		"autoprefixer": "^10.4.4",
		"concurrently": "^7.0.0",
		"eslint": "^8.11.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "7.0.4",
		"jest": "^27.5.1",
		"lint-staged": "12.3.5",
		"postcss": "^8.4.12",
		"prettier": "^2.5.1",
		"tailwindcss": "^3.0.23",
		"ts-jest": "^27.1.3",
		"typescript": "^4.5.5"
	},
	"engines": {
		"node": ">=14"
	}
}
